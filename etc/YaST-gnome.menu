	<DefaultMergeDirs/>

   <Layout>
     <Menuname>Software</Menuname>
     <Menuname>Hardware</Menuname>
     <Menuname>System</Menuname>
     <Menuname>Network Devices</Menuname>
     <Menuname>Network Services</Menuname>
     <Menuname>AppArmor</Menuname>
